@model Movie
@{
    ViewData["Title"] = "Home Page";
}
<head>
    <title></title>
    <link href="/css/style.css" rel="stylesheet"/>
</head>
<body>
<h1>Movie List</h1>
<div id="movies">
    <div class="movie">
        <p>@Model.Title</p>
        <p>@Model.Description</p>
        <p>@Model.Director</p>
        <p>
            <ul>
                @foreach (var actor in Model.Actors)
                {
                    <li>@actor</li>
                }
            </ul>
        </p>
    </div>
</div>
</body>

